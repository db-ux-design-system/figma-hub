  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/node": "^25.3.0",
    "adm-zip": "^0.5.16",
    "npm-run-all2": "8.0.4"
  },
  "dependencies": {
    "@db-ux/db-theme": "5.3.1",
    "@db-ux/db-theme-fonts": "2.0.5",
    "@db-ux/db-theme-icons": "3.6.0",
