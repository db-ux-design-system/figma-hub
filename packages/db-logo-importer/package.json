{
  "name": "@db-ux/logo-importer",
  "private": true,
  "version": "0.1.0",
  "description": "This plugin creates a Figma component from an imported SVG, assigns color variables to shapes, and scales it to a defined size.",
  "main": "index.js",
  "workspaces": [
    "plugin/*",
    "ui/*"
  ],
  "scripts": {
    "build": "npm-run-all --parallel build:*",
    "build:code": "npm run build -w=db-logo-importer-code -- --minify",
    "build:ui": "npm run build -w=db-logo-importer-ui",
    "dev": "npm-run-all --parallel dev:*",
    "dev:code": "npm run dev -w=db-logo-importer-code",
    "dev:ui": "npm run dev -w=db-logo-importer-ui"
  }
}
