{
  "name": "db-logo-importer",
  "version": "0.1.0",
  "description": "Figma plugin to create components from SVGs, assign color variables, and scale.",
  "main": "code.js",
  "scripts": {
    "build:ui": "vite build",
    "build:code": "vite build --config vite.config.code.ts",
    "build": "npm run build:ui && npm run build:code",
    "dev": "concurrently \"vite build --watch --emptyOutDir false\" \"vite build --config vite.config.code.ts --watch --emptyOutDir false\""
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@figma/plugin-typings": "^1.121.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "autoprefixer": "^10.4.23",
    "concurrently": "^9.2.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "vite": "^7.3.0",
    "vite-plugin-singlefile": "^2.3.0"
  },
  "dependencies": {
    "@db-ux/db-theme": "^5.3.0",
    "@db-ux/react-core-components": "4.2.6",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  }
}
