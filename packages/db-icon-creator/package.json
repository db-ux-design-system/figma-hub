{
  "name": "db-icon-creator",
  "private": true,
  "version": "0.1.0",
  "description": "Figma plugin to validate and edit icon component sets according to DB guidelines.",
  "main": "index.js",
  "workspaces": [
    "plugin",
    "ui"
  ],
  "scripts": {
    "build": "npm-run-all --parallel build:*",
    "build:code": "npm run build -w=db-icon-creator-code -- --minify",
    "build:ui": "npm run build -w=db-icon-creator-ui",
    "dev": "npm-run-all --parallel dev:*",
    "dev:code": "npm run dev -w=db-icon-creator-code",
    "dev:ui": "npm run dev -w=db-icon-creator-ui",
    "test": "npm run test -w=db-icon-creator-code",
    "test:watch": "npm run test:watch -w=db-icon-creator-code"
  },
  "author": "DB Systel GmbH",
  "license": "Apache-2.0"
}
